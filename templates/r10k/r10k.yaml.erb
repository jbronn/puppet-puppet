# Generated by puppet::r10k::config, do not modify.
---
<%
def quote_setting(arg)
  if arg.is_a?(String)
    "'#{arg}'"
  else
    arg
  end
end
-%>
<% if @postrun.count > 0 -%>
postrun: [<%= (@postrun.collect { |cmd| "\"#{cmd}\"" }).join(', ') %>]
<% end -%>
<% if @cachedir -%>
cachedir: <%= @cachedir %>
<% end -%>
<% if @sources.count > 0-%>
sources:
<%   @sources.keys.sort.each do |source| -%>
<%     source_data = @sources[source]
       if ! source_data.empty? -%>
  <%= source -%>:
<%       source_data.sort.each do |key, value| -%>
    <%= key -%>: <%= quote_setting(value) %>
<%       end -%>
<%     end -%>
<%   end -%>
<% end -%>
<% if @forge.count > 0 -%>
forge:
<%   @forge.sort.each do |key, value| -%>
  <%= key -%>: <%= quote_setting(value) %>
<%   end -%>
<% end -%>
<% if @git.count > 0 -%>
git:
<%   @git.sort.each do |key, value| -%>
  <%= key -%>: <%= quote_setting(value) %>
<%   end -%>
<% end -%>
